{
  "compilerOptions": {
   "target": "ES2022",
    "module": "ESNext",
    "lib": ["ES2023", "DOM"],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,

    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
